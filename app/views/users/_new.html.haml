= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f|
  = f.label :email
  = f.email_field :email, class: 'text-input form-control', autofocus: true
  = f.label :password
  = f.password_field :password, class: 'text-input form-control'
  = f.submit t('user.login'), class: 'btn btn-default'