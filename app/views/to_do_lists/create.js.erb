var options = {
    <% if @list.persisted? %>
        html: "<%=j render partial: 'list', locals: {list: @list} %>",
        message: "<%=j render partial: 'create_success' %>",
        id: <%= @list.id %>,
    <% else %>
        error: true,
    <% end %>
        per_page: 3,
        on_page: 4
};

var todoList = new TodoList();
todoList.onCreate(options);