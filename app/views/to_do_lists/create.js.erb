var options = {
    <% if @list.persisted? %>
        html: "<%=j render partial: 'list', locals: {list: @list} %>",
        message: "<%=j render partial: 'create_success', locals: {page: @pager[:on_page]} %>",
        per_page: <%= @pager[:per_page] %>,
        id: <%= @list.id %>,
    <% else %>
        error: true
    <% end %>
};

var todoList = new TodoList();
todoList.onCreate(options);