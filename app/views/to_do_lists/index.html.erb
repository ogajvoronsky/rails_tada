<h1> <%= t 'list.all' %></h1>
<input id="add-list-button" class="btn btn-warning" type="button" value="<%= t 'list.new.create_button' %>">
<hr/>

<%= render partial: 'new' %>

<% @lists.each do |list| %>
    <div class="list" id="list-<%= list.id %>">
      <%= render partial: 'article', locals: {list: list} %>
    </div>
<% end %>